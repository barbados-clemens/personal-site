{
  "name": "calebukle-com",
  "version": "3.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "netlify dev",
    "build": "ng build && scully --nw -- --scanRoutes",
    "move": "mkdir -p public && cp -rf dist/static/* public",
    "build:prod": "ng build --prod && scully --nw -- --scanRoutes",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --watch=false --code-coverage --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scully": "scully",
    "scully:s": "scully serve",
    "scully:serve": "scully serve",
    "b:s": "npm run build && npm run scully:s",
    "cf": "cd workers && lerna run deploy"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint -- --fix",
      "pre-push": "npm run lint && npm run test:ci",
      "post-checkout": "npm install"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.3",
    "@angular/cdk": "^11.2.2",
    "@angular/common": "~11.2.3",
    "@angular/compiler": "~11.2.3",
    "@angular/core": "~11.2.3",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "~11.2.3",
    "@angular/platform-browser": "~11.2.3",
    "@angular/platform-browser-dynamic": "~11.2.3",
    "@angular/router": "~11.2.3",
    "@angular/service-worker": "^11.2.3",
    "@barbados-clemens/scully-plugin-algolia": "^0.0.29",
    "@barbados-clemens/scully-plugin-blur-up-images": "^0.0.30",
    "@barbados-clemens/scully-plugin-firebase-likes": "^0.0.28",
    "@barbados-clemens/scully-plugin-header-links": "^0.0.28",
    "@scullyio/init": "^1.0.0-beta.0",
    "@scullyio/ng-lib": "^1.0.0-beta.0",
    "@scullyio/scully": "^1.0.0-beta.0",
    "@sentry/browser": "^5.20.1",
    "algoliasearch": "^4.3.1",
    "firebase": "^8.2.9",
    "netlify-cms": "^2.10.54",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1102.2",
    "@angular-devkit/build-angular": "~0.1102.2",
    "@angular/cli": "~11.2.2",
    "@angular/compiler-cli": "~11.2.3",
    "@angular/language-service": "^11.2.3",
    "@cloudflare/wrangler": "^1.10.3",
    "@sentry/cli": "^1.55.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.53",
    "@types/sharp": "^0.25.1",
    "axios": "^0.19.2",
    "codelyzer": "^6.0.0",
    "dotenv": "^8.2.0",
    "firebase-admin": "^9.0.0",
    "firebase-tools": "^8.0.0",
    "fuzzy": "^0.1.3",
    "husky": "^4.2.5",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jsdom": "^16.3.0",
    "karma": "~6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
