{
  "name": "calebukle-com",
  "version": "2.0.0-beta",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && scully --nw --scan",
    "move": "mkdir -p public && cp -rf dist/static/* public",
    "build:prod": "ng build --prod && scully --nw --scan",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --watch=false --code-coverage --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scully": "scully",
    "scully:s": "scully serve",
    "b:s": "npm run build && npm run scully:s",
    "cf": "cd cf-workers && wranger publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint -- --fix",
      "pre-push": "npm run lint && npm run test:ci",
      "post-checkout": "npm install"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.3",
    "@angular/cdk": "^9.2.1",
    "@angular/common": "~9.1.3",
    "@angular/compiler": "~9.1.3",
    "@angular/core": "~9.1.3",
    "@angular/fire": "^6.0.0",
    "@angular/forms": "~9.1.3",
    "@angular/platform-browser": "~9.1.3",
    "@angular/platform-browser-dynamic": "~9.1.3",
    "@angular/router": "~9.1.3",
    "@angular/service-worker": "~9.1.3",
    "@scullyio/init": "0.0.25",
    "@scullyio/ng-lib": "latest",
    "@scullyio/scully": "latest",
    "@sentry/browser": "^5.12.1",
    "algoliasearch": "^4.0.2",
    "firebase": "^7.14.2",
    "netlify-cms": "^2.10.22",
    "netlify-plugin-rss": "0.0.5",
    "prismjs": "^1.19.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "xstate": "^4.7.8",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.900.0-0 || ^0.900.0",
    "@angular-devkit/build-angular": "~0.901.3",
    "@angular/cli": "~9.1.3",
    "@angular/compiler-cli": "~9.1.3",
    "@angular/language-service": "~9.1.3",
    "@cloudflare/wrangler": "^1.8.4",
    "@gammastream/scully-plugin-sitemap": "0.0.4",
    "@notiz/scully-plugin-fouc": "^0.1.0",
    "@notiz/scully-plugin-lazy-images": "^0.1.0",
    "@sentry/cli": "^1.51.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/prismjs": "^1.16.0",
    "codelyzer": "^5.1.2",
    "dotenv": "^8.2.0",
    "firebase-admin": "^8.9.2",
    "firebase-tools": "^7.12.1",
    "fuzzy": "^0.1.3",
    "husky": "^4.2.2",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jsdom": "^16.2.2",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "netlify-plugin-a11y": "0.0.6",
